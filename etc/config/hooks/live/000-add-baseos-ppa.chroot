#!/bin/sh
# Description: Add The PikaOS BaseOS PPA

rm -rfv /etc/apt/sources.list.d/pika.list || echo "Failed to remove os-builder archives list" && exit 1

apt update

apt upgrade --allow-downgrades --yes --option Acquire::Retries=5 --option Acquire::http::Timeout=100 --option Dpkg::Options::="--force-confdef" 
